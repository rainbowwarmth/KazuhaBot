{
    "desc": "命令总览json,按照顺序进行匹配",
    "command": {
        "admin": {
            "ping": {
                "reg": "^#ping$",
                "fnc": "ping",
                "type": ["DIRECT"],
                "describe": "检测数据库是否正常"
            },
            "status": {
                "reg": "^#(状态|status)$",
                "fnc": "status",
                "type": ["GUILD", "DIRECT"],
                "describe": "查询bot状态"
            }
        },
        "help": {
            "helpimage": {
                "reg": "^#help$",
                "fnc": "helpimage",
                "type": ["GUILD","DIRECT"],
                "describe": "help信息"
            },
            "commits": {
                "reg": "^#提交日志$",
                "fnc": "commits",
                "type": ["GUILD","DIRECT"],
                "describe": "help信息"
            },
            "info": {
                "reg": "^#版本$",
                "fnc": "info",
                "type": ["GUILD","DIRECT"],
                "describe": "help信息"
            }
        },
        "NewAPI": {
            "handleMessage": {
                "reg": "#(原神|绝区零|崩坏3)(公告|资讯|活动)[0-9]*$",
                "fnc": "handleMessage",
                "type": ["GUILD"],
                "describe": "test"
            }
        }
    }
}    
